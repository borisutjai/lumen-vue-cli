{
    "name": "rufusmaiwald/lumen-vue-cli",
    "description": "The Laravel Lumen Framework as backend API with Vue.js using vue-cli included",
    "keywords": ["framework", "laravel", "lumen", "vue", "vue-cli"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Rufus Maiwald",
            "email": "dev@rufusmaiwald.de",
            "homepage": "https://rufusmaiwald.de"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "laravel/lumen-framework": "^7.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
